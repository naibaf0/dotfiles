*foreground:        DRACULA_FG
*background:        DRACULA_BG

*color0:            DRACULA_BLACK
*color1:            DRACULA_RED
*color2:            DRACULA_GREEN
*color3:            DRACULA_YELLOW
*color4:            DRACULA_BLUE
*color5:            DRACULA_MAGENTA
*color6:            DRACULA_CYAN
*color7:            DRACULA_WHITE

*color8:            DRACULA_BRIGHT_BLACK
*color9:            DRACULA_BRIGHT_RED
*color10:           DRACULA_BRIGHT_GREEN
*color11:           DRACULA_BRIGHT_YELLOW
*color12:           DRACULA_BRIGHT_BLUE
*color13:           DRACULA_BRIGHT_MAGENTA
*color14:           DRACULA_BRIGHT_CYAN
*color15:           DRACULA_BRIGHT_WHITE

